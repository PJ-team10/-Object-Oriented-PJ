<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_main"
    app:startDestination="@id/entryFragment">

    <fragment
        android:id="@+id/entryFragment"
        android:name="com.example.object_oriented_pj_10.EntryFragment"
        android:label="Main"
        tools:layout="@layout/fragment_entry">
        <action
            android:id="@+id/action_entryFragment_to_sutdyFragment"
            app:destination="@id/sutdyFragment" />
        <action
            android:id="@+id/action_entryFragment_to_cookFragment"
            app:destination="@id/cookFragment" />
        <action
            android:id="@+id/action_entryFragment_to_exerciseFragment"
            app:destination="@id/exerciseFragment" />
    </fragment>
    <fragment
        android:id="@+id/sutdyFragment"
        android:name="com.example.object_oriented_pj_10.SutdyFragment"
        android:label="Study"
        tools:layout="@layout/fragment_study" />
    <fragment
        android:id="@+id/cookFragment"
        android:name="com.example.object_oriented_pj_10.CookFragment"
        android:label="Cook"
        tools:layout="@layout/fragment_cook" />
    <fragment
        android:id="@+id/exerciseFragment"
        android:name="com.example.object_oriented_pj_10.ExerciseFragment"
        android:label="Exercise"
        tools:layout="@layout/fragment_exercise" />
</navigation>
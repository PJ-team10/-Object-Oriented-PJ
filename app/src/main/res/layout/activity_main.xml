<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity"
    android:background="@drawable/backgroundimage"
    >

    <TextView
        android:id="@+id/textView"
        android:layout_width="320dp"
        android:layout_height="693dp"
        android:background="@color/white"
        android:textAppearance="@style/TextAppearance.AppCompat.Display4"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.55"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.439" />

    <Button
        android:id="@+id/SetTime"
        android:layout_width="200dp"
        android:layout_height="37dp"
        android:backgroundTint="#03A9F4"
        android:text="해당 운동 시간을 정하세요."
        android:backgrou
        tools:layout_editor_absoluteX="137dp"
        tools:layout_editor_absoluteY="292dp" />

    <EditText
        android:id="@+id/editTextSetsToExercise"
        android:layout_width="219dp"
        android:layout_height="48dp"
        android:ems="10"
        android:hint="운동 이름을 적으세요"
        android:inputType="textPersonName"
        android:minHeight="48dp"
        tools:layout_editor_absoluteX="85dp"
        tools:layout_editor_absoluteY="673dp" />

    <Button
        android:id="@+id/button3"
        android:layout_width="149dp"
        android:layout_height="73dp"
        android:backgroundTint="#03A9F4"
        android:text="설정이 끝났습니다."
        tools:layout_editor_absoluteX="121dp"
        tools:layout_editor_absoluteY="478dp" />

    <Button
        android:id="@+id/SetCount"
        android:layout_width="198dp"
        android:layout_height="0dp"
        android:backgroundTint="#03A9F4"
        android:text="해당 운동 세트 수를 정하세요"
        tools:layout_editor_absoluteX="121dp"
        tools:layout_editor_absoluteY="232dp" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/floatingActionButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:clickable="true"
        app:backgroundTint="#6F8FEF"
        app:srcCompat="@android:drawable/ic_menu_add"
        app:useCompatPadding="false"
        tools:ignore="SpeakableTextPresentCheck"
        tools:layout_editor_absoluteX="177dp"
        tools:layout_editor_absoluteY="571dp" />

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">


    </LinearLayout>

    <TextView
        android:id="@+id/SetBox"
        android:layout_width="266dp"
        android:layout_height="231dp"
        android:background="@drawable/edge"
        android:text="TextView"
        tools:layout_editor_absoluteX="71dp"
        tools:layout_editor_absoluteY="232dp" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="67dp"
        android:layout_height="25dp"
        app:srcCompat="@drawable/addbutton"
        tools:layout_editor_absoluteX="166dp"
        tools:layout_editor_absoluteY="170dp" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="96dp"
        android:layout_height="92dp"
        app:srcCompat="@drawable/exercise"
        tools:layout_editor_absoluteX="61dp"
        tools:layout_editor_absoluteY="103dp" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="89dp"
        android:layout_height="34dp"
        app:srcCompat="@drawable/startbutton"
        tools:layout_editor_absoluteX="270dp"
        tools:layout_editor_absoluteY="103dp" />

</androidx.constraintlayout.widget.ConstraintLayout>